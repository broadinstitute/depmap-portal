  "correlation-structure-of-dependencies": {
    "plot_type": "correlation_heatmap",
    "index_type": "depmap_model",
    "dimensions": {
      "x": {
        "axis_type": "aggregated_slice",
        "slice_type": "gene",
        "dataset_id": "Chronos_Combined",
        "aggregation": "correlation",
        "context": {
          "context_type": "gene",
          "expr": {
            "in": [
              {
                "var": "slice_label"
              },
              [
                "CAD",
                "CMPK1",
                "CMPK2",
                "CTPS1",
                "CTPS2",
                "DHODH",
                "NME1",
                "NME2",
                "NME1-NME2",
                "NME4",
                "NME6",
                "UMPS"
              ]
            ]
          },
          "name": "DeNovoPyrimidine"
        }
      }
    }
  }
}
