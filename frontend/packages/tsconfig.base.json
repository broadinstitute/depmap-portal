{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "lib": ["es6", "es2017", "dom", "dom.iterable"],
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declarationDir": "../node_modules/.cache/depmap",
    "paths": {
      "react": ["../../node_modules/@types/react"],
      "react-dom": ["../../node_modules/@types/react-dom"]
    }
  }
}
